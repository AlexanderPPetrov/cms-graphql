{
    "name": "cms",
    "version": "1.0.0",
    "private": true,
    "main": "build/server.js",
    "scripts": {
        "clean": "rimraf build && mkdir build",
        "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/server.js",
        "build": "npm run build:client && npm run build:server",
        "build-babel": "babel -d ./build ./src -s",
        "build:client": "nuxt build",
        "build:server": "npm run clean && npm run build-babel",
        "start": "cross-env NODE_ENV=production node build/server.js",
        "generate": "nuxt generate"
    },
    "dependencies": {
        "@nuxtjs/apollo": "^4.0.1-rc.5",
        "@nuxtjs/axios": "^5.12.4",
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.5.2",
        "bootstrap-icons": "^1.2.1",
        "bootstrap-vue": "^2.17.3",
        "class-validator": "^0.12.2",
        "consola": "^2.15.0",
        "core-js": "^3.6.5",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-graphql": "^0.12.0",
        "express-jwt": "^6.0.0",
        "graphql": "^15.4.0",
        "graphql-tag": "^2.11.0",
        "graphql-tools": "^7.0.2",
        "jsonwebtoken": "^8.5.1",
        "merge-graphql-schemas": "^1.7.8",
        "mongoose": "^5.11.7",
        "node-sass": "^5.0.0",
        "nuxt": "^2.14.6",
        "nuxt-i18n": "^6.16.0",
        "reflect-metadata": "^0.1.13",
        "sass-loader": "^10.1.0",
        "type-graphql": "^1.1.1",
        "validator": "^13.5.2",
        "vuelidate": "^0.7.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/node": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/register": "^7.12.10",
        "@nuxt/types": "^2.14.12",
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.6",
        "rimraf": "^3.0.2"
    }
}
